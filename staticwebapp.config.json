{
    "routes": [
        {
            "route": "/data/*",
            "headers": {
                "Cache-Control": "public, max-age=3600"
            }
        },
        {
            "route": "/css/*",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        },
        {
            "route": "/js/*",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        }
    ],
    "globalHeaders": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Permissions-Policy": "camera=(), microphone=(), geolocation=()",
        "Content-Security-Policy": "default-src 'none'; script-src 'self' https://cdnjs.cloudflare.com; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self'; connect-src 'self'; frame-ancestors 'none'; base-uri 'self'; form-action 'none'"
    },
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}
